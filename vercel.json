{
  "version": 2,
  "rewrites": [
    {
      "source": "/",
      "destination": "/BUSINESS_PROCESS_MANUAL.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}

